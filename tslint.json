{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
		"indent": [false]
	},
	"rules": {
		"arrow-parens": [false],
		"comment-format": [false],
		"indent": [false],
		"object-literal-shorthand": [false],
		"object-literal-sort-keys": [false],
		"no-bitwise": [false],
		"no-conditional-assignment": [false],
		"trailing-comma": [false]
	},
	"rulesDirectory": []
}
